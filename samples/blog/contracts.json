            }
          },
          "categories": {
            "kind": "array",
            "items": {
              "kind": "any"
            }
          },
          "excerpt": {
            "kind": "scalar",
            "string": true,
            "integer": true,
            "null": false,
            "allow_empty": true
          },
          "published_at": {
            "kind": "scalar",
            "string": true,
            "integer": true,
            "null": false,
            "allow_empty": true
