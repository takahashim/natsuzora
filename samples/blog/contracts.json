        "featured",
        "heading",
        "introduction"
      ],
      "properties": {
        "featured": {
          "kind": "object",
          "required": [
            "excerpt",
            "title",
            "url"
          ],
          "properties": {
            "excerpt": {
              "kind": "scalar",
              "string": true,
              "integer": true,
              "null": false,
              "allow_empty": true
            },
            "title": {
              "kind": "scalar",
